{% extends 'ComprasBundle::base.html.twig' %}

{% block body %}
    <div class="container">

        <h1>EbNfeItem</h1>

        <table class="record_properties">
            <tbody>
            <tr>
                <th>Idnfeitem</th>
                <td>{{ entity.idNfeItem }}</td>
            </tr>
            <tr>
                <th>Nome</th>
                <td>{{ entity.nome }}</td>
            </tr>
            <tr>
                <th>Descricao</th>
                <td>{{ entity.descricao }}</td>
            </tr>
            <tr>
                <th>Cor</th>
                <td>{{ entity.cor }}</td>
            </tr>
            <tr>
                <th>Valor</th>
                <td>{{ entity.valor }}</td>
            </tr>
            <tr>
                <th>Icms</th>
                <td>{{ entity.icms }}</td>
            </tr>
            <tr>
                <th>Ipi</th>
                <td>{{ entity.ipi }}</td>
            </tr>
            <tr>
                <th>Pis</th>
                <td>{{ entity.pis }}</td>
            </tr>
            <tr>
                <th>Dtcadastro</th>
                <td>{{ entity.dtCadastro|date('Y-m-d H:i:s') }}</td>
            </tr>
            </tbody>
        </table>

        <ul class="record_actions">
            <li>
                <a href="{{ path('compras_nfe_item') }}">
                    Back to the list
                </a>
            </li>
            <li>
                <a href="{{ path('compras_nfe_item_edit', { 'id': entity.idNfeItem }) }}">
                    Edit
                </a>
            </li>
            <li>{{ form(delete_form) }}</li>
        </ul>
    </div>
{% endblock %}
