<?php

namespace AutenticacaoBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * EbTemperatura
 *
 * @ORM\Table(name="eb_temperatura")
 * @ORM\Entity
 */
class EbTemperatura
{
    /**
     * @var integer
     *
     * @ORM\Column(name="id_temperatura", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $idTemperatura;

    /**
     * @var string
     *
     * @ORM\Column(name="nome", type="string", length=45, nullable=true)
     */
    private $nome;

    /**
     * @var string
     *
     * @ORM\Column(name="descricao", type="string", length=255, nullable=true)
     */
    private $descricao;

    /**
     * @var string
     *
     * @ORM\Column(name="ativo", type="string", length=1, nullable=false)
     */
    private $ativo = '1';


}
