{% extends 'ComprasBundle::base.html.twig' %}

{% block title %}Elever-PÃ¡gina Configuracoes{% endblock %}

{% block body %}
    <div class="container">

    <h1>EbEmpresa list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Idempresa</th>
                <th>Nome</th>
                <th>Logradouro</th>
                <th>Bairro</th>
                <th>Cidade</th>
                <th>Uf</th>
                <th>Ativo</th>
                <th>Excluido</th>
                <th>Dtcadastro</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('configuracao_empresa_show', { 'id': entity.idEmpresa }) }}">{{ entity.idEmpresa }}</a></td>
                <td>{{ entity.nome }}</td>
                <td>{{ entity.logradouro }}</td>
                <td>{{ entity.bairro }}</td>
                <td>{{ entity.cidade }}</td>
                <td>{{ entity.uf }}</td>
                <td>{{ entity.ativo }}</td>
                <td>{{ entity.excluido }}</td>
                <td>{% if entity.dtCadastro %}{{ entity.dtCadastro|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('configuracao_empresa_show', { 'id': entity.idEmpresa }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('configuracao_empresa_edit', { 'id': entity.idEmpresa }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('configuracao_empresa_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    </div>
    {% endblock %}
